{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-var-requires": false,
    "max-line-length": [
      true,
      180
    ],
    "no-console": false,
    "object-literal-sort-keys": false,
    "only-arrow-functions": [
      true,
      "allow-declarations",
      "allow-named-functions"
    ]
  },
  "rulesDirectory": []
}